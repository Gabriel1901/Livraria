<?php

namespace LivrariaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Description of Genero
 *
 * @author Gabriel
 * 
 * @ORM\Entity
 * @ORM\Table(name="genero")
 */
class Genero
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    
    /**
     * @ORM\Column(type="integer")
     */
    private $nome;
}
